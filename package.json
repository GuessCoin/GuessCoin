    "@material-tailwind/react": "0.3.4",
    "@noble/secp256k1": "^1.7.0",
    "@react-chess/chessground": "^1.3.4",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.33",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "bech32": "^2.0.0",
    "buffer": "^6.0.3",
    "chess.js": "^0.13.4",
    "dexie": "^3.2.2",
    "dexie-react-hooks": "^1.1.1",
    "nostr-tools": "^0.23.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.3.0",
    "react-scripts": "^5.0.1",
    "typescript": "^4.6.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "prettier --check --no-error-on-unmatched-pattern 'src/**/*.{ts,tsx,json,css,md}'",
    "format": "prettier --write --no-error-on-unmatched-pattern 'src/**/*.{ts,tsx,json,css,md}'",
    "regtest:clear": "docker-compose --file docker/regtest/docker-compose.yml down --volumes --remove-orphans",
    "regtest:up": "docker-compose --file docker/regtest/docker-compose.yml up",
    "regtest:down": "docker-compose --file docker/regtest/docker-compose.yml down"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "prettier": {
    "printWidth": 120,
    "semi": false,
    "singleQuote": true
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "jest-watch-typeahead": "^0.6.5",
    "prettier": "^2.8.1",
    "stockfish": "^14.1.0"
  }
